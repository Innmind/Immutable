{
    "name": "innmind/immutable",
    "type": "library",
    "description": "Immutable PHP primitive wrappers",
    "keywords": ["wrapper", "immutable"],
    "homepage": "http://github.com/Innmind/Immutable",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Immutable/issues"
    },
    "require": {
        "php": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Immutable\\": "src/",
            "Fixtures\\Innmind\\Immutable\\": "fixtures/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Immutable\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "vimeo/psalm": "~3.8",
        "innmind/black-box": "^4.1"
    },
    "conflict": {
        "innmind/black-box": "<4.1|~5.0",
        "vimeo/psalm": "3.11.2|3.11.1|3.11.0"
    },
    "suggest": {
        "innmind/black-box": "For property based testing"
    },
    "provide": {
        "innmind/black-box": "4.1.0"
    }
}
